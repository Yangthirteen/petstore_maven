<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="persistence.AccountDAO">
    <select id="selectAccountByUsernameAndPassword" parameterType="domain.Account" resultType="domain.Account">
        select * from b_user where username = #{username} and password = #{password}
    </select>
</mapper>